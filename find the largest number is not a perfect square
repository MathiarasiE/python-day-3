arr = [16, 20, 25, 2, 3, 10]
n = len(arr)


largest_not_perfect_square = -1


for i in range(n):
 
    if not (int(arr[i]**0.5)**2 == arr[i]):
       
        if arr[i] > largest_not_perfect_square:
            largest_not_perfect_square = arr[i]

print("The largest number that is not a perfect square is:", largest_not_perfect_square)
